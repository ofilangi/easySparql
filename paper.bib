	pmid = {19483092},
	pmcid = {PMC2703982},
	keywords = {Abstracting and Indexing, Biomedical Research, Internet, Natural Language Processing, Software, Systems Integration, User-Computer Interface, Vocabulary, Controlled},
	pages = {W170--173},
	file = {Texte intégral:/home/ofilangi/snap/zotero-snap/common/Zotero/storage/5N5NKKHT/Noy et al. - 2009 - BioPortal ontologies and integrated data resource.pdf:application/pdf},
