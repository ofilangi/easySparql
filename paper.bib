	month = feb,
	year = {2006},
	pmid = {16507094},
	pmcid = {PMC1420335},
	keywords = {Database Management Systems, Databases, Factual, Information Storage and Retrieval, Natural Language Processing, Programming Languages, Software, User-Computer Interface, Vocabulary, Controlled},
	pages = {97},
	file = {Texte intégral:/home/ofilangi/snap/zotero-snap/common/Zotero/storage/73C3Q54G/Côté et al. - 2006 - The Ontology Lookup Service, a lightweight cross-p.pdf:application/pdf},
